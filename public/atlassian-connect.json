{
   "key": "com.thefuntasty.ghplugin",
   "name": "GitHub App Plugin for JIRA",
   "description": "GitHub Add-on for creating branches",
   "vendor": {
      "name": "T Osowski",
      "url": "https://github.com"
   },
   "baseUrl": "https://ancient-mesa-93229.herokuapp.com",
   "authentication": {
      "type": "none"
   },
   "modules": {
      "webPanels": [
         {
            "url": "/main_entry?issueKey={issue.key}",
            "key": "funtasty-gh-plugin",
            "location": "atl.jira.view.issue.right.context",
            "name": {
               "value": "GitHub Development"
            }
         }
      ]
   }
}
